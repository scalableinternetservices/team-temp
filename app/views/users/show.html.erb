<% provide(:title, "Profile") %>

<% content_for :head do %>
  <%= stylesheet_link_tag "user_profile" %>
<% end %>

<!DOCTYPE html>
<div class="username">
    Welcome, <%= @user.username %>!
</div>

<div class="questions">
    <% @questions.each do |question| %>
        <div class="question_description">
            question.description
        </div>
    <% end %>
</div>